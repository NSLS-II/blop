{
    "disable_existing_loggers": false,
    "filters": {
        "principal": {
            "()": "tiled.server.principal_log_filter.PrincipalFilter"
        },
        "correlation_id": {
            "()": "asgi_correlation_id.CorrelationIdFilter",
            "default_value": "-",
            "uuid_length": 16
        }
    },
    "formatters": {
        "access": {
            "()": "uvicorn.logging.AccessFormatter",
            "datefmt": "%Y-%m-%dT%H:%M:%S",
            "format": "[%(correlation_id)s] %(client_addr)s (%(principal)s) - '%(request_line)s' %(status_code)s",
            "use_colors": true
        },
        "default": {
            "()": "uvicorn.logging.DefaultFormatter",
            "datefmt": "%Y-%m-%dT%H:%M:%S",
            "format": "[%(correlation_id)s] %(levelprefix)s %(message)s",
            "use_colors": true
        }
    },
    "handlers": {
        "access": {
            "class": "logging.StreamHandler",
            "filters": [
                "principal",
                "correlation_id"
            ],
            "formatter": "access",
            "stream": "ext://sys.stdout"
        },
        "default": {
            "class": "logging.StreamHandler",
            "filters": [
                "correlation_id"
            ],
            "formatter": "default",
            "stream": "ext://sys.stderr"
        }
    },
    "loggers": {
        "uvicorn.access": {
            "handlers": [
                "access"
            ],
            "level": "ERROR",
            "propagate": false
        },
        "uvicorn.error": {
            "handlers": [
                "default"
            ],
            "level": "ERROR",
            "propagate": false
        }
    },
    "version": 1
}